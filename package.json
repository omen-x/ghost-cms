{
  "name": "ghost-cms",
  "version": "0.0.0-dev",
  "main": "src/server/index.ts",
  "scripts": {
    "dev-server": "ts-node-dev --inspect --no-notify src/server",
    "lint-server": "eslint --quiet --fix --ext .js,.ts src/server",
    "lint": "eslint --quiet --fix --ext .js,.ts src/",
    "build-client": "npm run lint-client && tsc --noEmit && webpack --mode=production",
    "build-server": "rm -rf build/server/**/* && mkdir -p build/server && npm run lint-server && tsc --outDir build/srver/",
    "build": "npm run build-server",
    "start": "node build/server/index.js"
  },
  "author": "Denis Volok (denis.omen.x@gmail.com)",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "csv-parse": "^4.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "mongoose": "^5.8.1",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/bcrypt": "^3.0.0",
    "@types/connect-redis": "0.0.13",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/express-list-endpoints": "^4.0.1",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "0.0.45",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.5.34",
    "@types/multer": "^1.4.2",
    "@types/node": "^12.12.21",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.14",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-security": "^1.4.0",
    "express-list-endpoints": "^4.0.1",
    "husky": "^3.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
